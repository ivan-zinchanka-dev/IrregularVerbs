<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:IrregularVerbs.Converters"
                    xmlns:configs="clr-namespace:IrregularVerbs.Models.Configs">
    
    <converters:AnswerResultToBrushConverter x:Key="AnswerResultToBrushConverter"/>
    <converters:AnswerResultToInfoConverter x:Key="AnswerResultToInfoConverter"/>
    
    <converters:EnumToInt32Converter x:Key="EnumToNumberConverter"/>
    
    <configs:ApplicationSettings x:Key="ApplicationSettings" 
                                NativeLanguage="Russian"
                                VerbsCount="10"
                                AlphabeticalOrder="True"/>
    
</ResourceDictionary>